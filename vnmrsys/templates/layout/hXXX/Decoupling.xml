          />
        <entry loc="120 25" size="80 20"
          style="PlainText"
          vq="pwHwaltz"
          vc="pwHwaltz=$VALUE"
          set="$VALUE=pwHwaltz"
          digits="2"
          />
        <label loc="210 25" size="30 20"
          style="Label1"
          label="µs"
          />
      </group>
    </group>

