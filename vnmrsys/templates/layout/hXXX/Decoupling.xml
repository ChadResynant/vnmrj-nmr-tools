<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
  hXXX Decoupling Tab
  H, Y, and Z channel decoupling parameters
-->
<template name="Decoupling" element="pages" type="acquisition" >
  <group size="750 450"
    style="Heading1"
    label="Decoupling"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;0)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Decoupling"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >

    <!-- H Channel Decoupling -->
    <group loc="10 10" size="360 200"
      style="Heading2"
      label="H Channel Decoupling"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="10 20" size="100 20"
        style="Label1"
        label="Sequence"
        justify="Left"
        />
      <menu loc="120 20" size="120 20"
        style="PlainText"
        vq="Hseq"
        vc="parunits('set','Hseq','$VALUE')"
        set="$VALUE=Hseq"
        editable="No"
        tooltip="H decoupling sequence type"
        >
        <mlabel label="none" chval="none" />
        <mlabel label="TPPM" chval="tppm" />
        <mlabel label="SPINAL" chval="spinal" />
        <mlabel label="WALTZ" chval="waltz" />
      </menu>

      <!-- TPPM Parameters -->
      <group loc="0 50" size="360 100"
        vq="Hseq"
        show="if Hseq='tppm' then $SHOW=1 else $SHOW=0 endif"
        >
        <label loc="10 0" size="100 20"
          style="Label1"
          label="Amplitude (aHtppm)"
          justify="Left"
          />
        <entry loc="120 0" size="80 20"
          style="PlainText"
          vq="aHtppm"
          vc="aHtppm=$VALUE"
          set="$VALUE=aHtppm"
          digits="0"
          />
        <label loc="210 0" size="60 20"
          style="Label1"
          label="(0-4095)"
          />

        <label loc="10 25" size="100 20"
          style="Label1"
          label="Pulse Width"
          justify="Left"
          />
        <entry loc="120 25" size="80 20"
          style="PlainText"
          vq="pwHtppm"
          vc="pwHtppm=$VALUE"
          set="$VALUE=pwHtppm"
          digits="2"
          />
        <label loc="210 25" size="30 20"
          style="Label1"
          label="µs"
          />

        <label loc="10 50" size="100 20"
          style="Label1"
          label="Phase Angle"
          justify="Left"
          />
        <entry loc="120 50" size="80 20"
          style="PlainText"
          vq="phHtppm"
          vc="phHtppm=$VALUE"
          set="$VALUE=phHtppm"
          digits="1"
          />
        <label loc="210 50" size="30 20"
          style="Label1"
          label="deg"
          />
      </group>

      <!-- SPINAL Parameters -->
      <group loc="0 50" size="360 125"
        vq="Hseq"
        show="if Hseq='spinal' then $SHOW=1 else $SHOW=0 endif"
        >
        <label loc="10 0" size="100 20"
          style="Label1"
          label="Amplitude (aHspinal)"
          justify="Left"
          />
        <entry loc="120 0" size="80 20"
          style="PlainText"
          vq="aHspinal"
          vc="aHspinal=$VALUE"
          set="$VALUE=aHspinal"
          digits="0"
          />

        <label loc="10 25" size="100 20"
          style="Label1"
          label="Pulse Width"
          justify="Left"
          />
        <entry loc="120 25" size="80 20"
          style="PlainText"
          vq="pwHspinal"
          vc="pwHspinal=$VALUE"
          set="$VALUE=pwHspinal"
          digits="2"
          />
        <label loc="210 25" size="30 20"
