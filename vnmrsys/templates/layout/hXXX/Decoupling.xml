          set="$VALUE=phHspinal"
          digits="1"
          />
        <label loc="210 50" size="30 20"
          style="Label1"
          label="deg"
          />

        <label loc="10 75" size="100 20"
          style="Label1"
          label="Alpha (alpHspinal)"
          justify="Left"
          />
        <entry loc="120 75" size="80 20"
          style="PlainText"
          vq="alpHspinal"
          vc="alpHspinal=$VALUE"
          set="$VALUE=alpHspinal"
          digits="1"
          />
        <label loc="210 75" size="30 20"
          style="Label1"
          label="deg"
          />
      </group>

      <!-- WALTZ Parameters -->
